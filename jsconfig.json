{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@app/*": ["src/app/*"],
      "@features/*": ["src/features/*"],
      "@shared/*": ["src/shared/*"],
      "@components/*": ["src/components/*"],
  "@context/*": ["src/context/*"],
  "@hooks/*": ["src/hooks/*"],
  "@controllers/*": ["src/controllers/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
      "@router/*": ["src/router/*"],
      "@assets/*": ["src/assets/*"]
    }
  },
  "exclude": ["node_modules", "backend", "dist"]
}
